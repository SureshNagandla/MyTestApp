    <!-- Enable Forms Authentication -->
    <authentication mode="Forms">
      <forms loginUrl="Pages/Public/Login.aspx" timeout="30"/>
    </authentication>
    <!-- Session timeout -->
    <sessionState timeout="30"/>
    <!-- Custom Error Pages -->
    <customErrors mode="On" defaultRedirect="~/Pages/Error/GenericError.aspx">
      <error statusCode="500" redirect="~/Pages/Error/GenericError.aspx"/>
    </customErrors>
    <compilation debug="true" targetFramework="4.8"/>
